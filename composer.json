{
  "name": "streak/streak-bundle",
  "type": "symfony-bundle",
  "description": "",
  "keywords": ["es","cqrs","event sourcing", "command query responsibility segregation", "command", "command bus", "event", "symfony", "bundle", "symfony bundle"],
  "homepage": "https://streakphp.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Alan Gabriel Bem",
      "email": "alan.bem@gmail.com",
      "homepage": "https://github.com/alanbem"
    }
  ],
  "autoload": {
    "psr-4": {
      "Streak\\StreakBundle\\": "src"
    }
  },
  "require": {
    "php": ">=7.1",
    "symfony/http-kernel": "^4.0",
    "symfony/form": "^4.0",
    "streak/streak": "dev-non-event-sourced-subscriptions as 1.0",
    "doctrine/dbal": "^2.8",
    "symfony/dependency-injection": "^4.0",
    "symfony/config": "^4.0",
    "symfony/yaml": "^4.0",
    "symfony/console": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "paragonie/random_compat": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
