{
  "name": "streak/streak-bundle",
  "type": "symfony-bundle",
  "description": "",
  "keywords": ["es","cqrs","event sourcing", "command query responsibility segregation", "command", "command bus", "event", "symfony", "bundle", "symfony bundle"],
  "homepage": "https://streakphp.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Alan Gabriel Bem",
      "email": "alan.bem@gmail.com",
      "homepage": "https://github.com/alanbem"
    }
  ],
  "autoload": {
    "psr-4": {
      "Streak\\StreakBundle\\": "src"
    }
  },
  "require": {
    "php": ">=7.4",
    "doctrine/dbal": "^2.13",
    "streak/streak": "dev-master as 1.0",
    "symfony/form": "^5.2",
    "symfony/config": "^5.2",
    "symfony/yaml": "^5.2",
    "symfony/console": "^5.2",
    "symfony/http-kernel": "^5.2",
    "symfony/dependency-injection": "^5.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.5.4",
    "friendsofphp/php-cs-fixer": "^3.0",
    "rector/rector": "^0.10.19"
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
